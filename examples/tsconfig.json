{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["../../packages/ai_model_application_suite/*"],
      "@ai-model-application-suite/core": ["../../packages/ai_model_application_suite/dist/index.d.ts"],
      "@ai-model-application-suite/core/*": ["../../packages/ai_model_application_suite/dist/*"]
    }
  },
  "include": ["**/*", "index.html"]
}
